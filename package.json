{
  "name": "trendie",
  "version": "0.4.5",
  "description": "CLI tool that displays Internet Explorer usage and trends for your site via Google Analytics.",
  "preferGlobal": true,
  "main": "./bin/trendie",
  "bin": {
    "trendie": "./bin/trendie"
  },
